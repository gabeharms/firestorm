<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<panel name="block_list_panel">
	<fs_block_list name="block_list">
		<fs_block_list.columns name="item_name" label="Nazwa" />
		<fs_block_list.columns name="item_type" label="Typ" />
	</fs_block_list>
	<layout_stack name="block_list_sidetray_main">
		<layout_panel name="back_button_container">
			<button tool_tip="Wróć do Ludzi" name="back" />
		</layout_panel>
		<layout_panel name="block_list_blocklaypanels">
			<layout_stack name="block_list_blocklaystack">
				<layout_panel name="block_panels_btnsort">
					<menu_button label="Sortuj..." name="view_btn" tool_tip="Opcje sortowania" />
				</layout_panel>
				<layout_panel name="block_panels_btnpersonobject">
					<menu_button label="Zablokuj..." name="plus_btn" tool_tip="Wybierz Rezydenta lub obiekt do zablokowania"/>
				</layout_panel>
				<layout_panel name="block_panels_btnunblock">
					<button label="Odblokuj" name="unblock_btn" tool_tip="Usuń Rezydenta lub obiekt z listy zablokowanych" />
				</layout_panel>
			</layout_stack>
		</layout_panel>
	</layout_stack>
</panel>
