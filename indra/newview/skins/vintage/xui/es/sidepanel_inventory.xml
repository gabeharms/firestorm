<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<panel label="Inventario" name="objects panel">
	<panel name="sidepanel__inventory_panel">
		<tab_container name="inventory_tab_container">
			<panel label="Inventario" name="panel_main_inventory"/>
			<panel label="Marketplace" name="inbox_outbox_layout_panel">
				<layout_stack name="inbox_outbox_layout_stack_off">
					<layout_panel name="inbox_layout_panel_off">
						<panel name="marketplace_inbox">
							<string name="InboxLabelWithArg">Items recibidos ([NUM])</string>
							<string name="InboxLabelNoArg">Items recibidos</string>
							<text value="Items recibidos" name="inbox_label"/>
							<text name="inbox_fresh_new_count">
								[NUM] Nuevo(s)
							</text>
							<panel name="inbox_inventory_placeholder_panel" tool_tip="Arrastra aquí items de tu inventario para gestionarlos y usarlos">
								<panel name="inbox_inventory_placeholder">
									Las compras realizadas en Marketplace serán entregadas aquí.
								</panel>
							</panel>
						</panel>
					</layout_panel>
					<layout_panel name="outbox_layout_panel">
						<panel name="marketplace_outbox">
							<string name="OutboxLabelWithArg">
								Merchant Outbox ([NUM])
							</string>
							<string name="OutboxLabelNoArg">
								Merchant Outbox
							</string>
							<text value="Merchant Outbox" name="outbox_label"/>
							<button tool_tip="Enviar a mi tienda en Marketplace" name="outbox_sync_btn"/>
							<panel name="outbox_inventory_placeholder_upperpanel">
								<panel name="outbox_inventory_placeholder_panel">
									<text name="outbox_inventory_placeholder_title">
										Cargando...
									</text>
								</panel>
							</panel>
						</panel>
					</layout_panel>
				</layout_stack>
			</panel>
		</tab_container>
	</panel>
</panel>
