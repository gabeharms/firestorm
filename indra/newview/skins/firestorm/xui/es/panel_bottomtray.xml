<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<panel name="bottom_tray">
	<string name="SpeakBtnToolTip" value="Activa/desactiva el micrófono"/>
	<string name="VoiceControlBtnToolTip" value="Muestra/oculta el panel de control de la voz"/>
	<layout_stack name="toolbar_stack">
		<layout_panel name="chat_bar_layout_panel">
			<button name="conversations_btn" tool_tip="Muestra/oculta la ventana de conversaciones"/>
			<button name="show_nearby_chat" tool_tip="Muestra/oculta la ventana de chat local"/>
		</layout_panel>
		<layout_panel name="gesture_panel">
			<gesture_combo_list name="Gesture" tool_tip="Muestra/oculta los gestos"/>
		</layout_panel>
		<layout_panel name="speak_panel">
			<speak_button label="Hablar" label_selected="Hablar" name="speak_btn"/>
		</layout_panel>
		<layout_panel name="bottom_sbhome">
			<bottomtray_button name="sidebar_home_btn" tool_tip="Muestra/oculta el Inicio de la barra lateral"/>
		</layout_panel>
		<layout_panel name="bottom_sbme">
			<bottomtray_button name="sidebar_me_btn" tool_tip="Muestra/oculta el perfil del avatar"/>
		</layout_panel>
		<layout_panel name="bottom_sbpeople">
			<bottomtray_button name="sidebar_people_btn" tool_tip="Muestra/oculta el panel Gente (Ctrl+Shift+A)"/>
		</layout_panel>
		<layout_panel name="bottom_sbplaces">
			<bottomtray_button name="sidebar_places_btn" tool_tip="Muestra/oculta el panel Lugares"/>
		</layout_panel>
		<layout_panel name="bottom_sbappearance">
			<bottomtray_button name="sidebar_appearance_btn" tool_tip="Muestra/oculta el panel Apariencia"/>
		</layout_panel>
		<layout_panel name="movement_panel">
			<bottomtray_button name="movement_btn" tool_tip="Muestra/oculta los controles de movimiento"/>
		</layout_panel>
		<layout_panel name="cam_panel">
			<bottomtray_button name="camera_btn" tool_tip="Muestra/oculta los controles de cámara"/>
		</layout_panel>
		<layout_panel name="snapshot_panel">
			<bottomtray_button name="snapshots" tool_tip="Tomar fotografías (Ctrl+Shift+S)"/>
		</layout_panel>
		<layout_panel name="build_btn_panel">
			<bottomtray_button name="build_btn" tool_tip="Muestra/oculta las Herramientas de Construcción (Ctrl+3)"/>
		</layout_panel>
		<layout_panel name="search_btn_panel">
			<bottomtray_button name="search_btn" tool_tip="Muestra/oculta la Búsqueda (Ctrl+F)"/>
		</layout_panel>
		<layout_panel name="world_map_btn_panel">
			<bottomtray_button name="world_map_btn" tool_tip="Muestra/oculta el Mapa Mundial (Ctrl+M)"/>
		</layout_panel>
		<layout_panel name="mini_map_btn_panel">
			<bottomtray_button name="mini_map_btn" tool_tip="Muestra/oculta el Minimapa (Ctrl+Shift+M)"/>
		</layout_panel>
		<layout_panel name="bottom_sbinv">
			<bottomtray_button name="sidebar_inv_btn" tool_tip="Muestra/oculta el Inventario (Ctrl+I)"/>
		</layout_panel>
		<layout_panel name="quickprefs_btn_panel">
			<bottomtray_button name="quickprefs_toggle" tool_tip="Muestra/oculta las Preferencias Rápidas"/>
		</layout_panel>
		<layout_panel name="ao_btn_panel">
			<bottomtray_button name="ao_toggle_btn" tool_tip="Activa/desactiva el Animation Overrider"/>
			<bottomtray_button name="ao_btn" tool_tip="Muestra/oculta el Animation Overrider"/>
		</layout_panel>
		<layout_panel name="im_well_panel">
			<chiclet_im_well name="im_well">
				<button name="Unread IM messages" tool_tip="Conversaciones"/>
			</chiclet_im_well>
		</layout_panel>
		<layout_panel name="notification_well_panel">
			<chiclet_notification name="notification_well">
				<button name="Unread" tool_tip="Notificaciones"/>
			</chiclet_notification>
		</layout_panel>
	</layout_stack>
</panel>
