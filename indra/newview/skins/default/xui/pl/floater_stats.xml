					<stat_bar label="Agenci - potomki" name="simchildagents"/>
					<stat_bar label="Obiekty" name="simobjects"/>
					<stat_bar label="Aktywne obiekty" name="simactiveobjects"/>
					<stat_bar label="Aktywne skrypty" name="simactivescripts"/>
