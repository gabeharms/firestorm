				<stat_bar label="Ping symulatora" name="ping"/>
			</stat_view>
			<stat_view label="Zaawansowane" name="advanced">
				<stat_view label="Rendering" name="render">
					<stat_bar label="KTris rysowane na kl." name="ktrisframe"/>
