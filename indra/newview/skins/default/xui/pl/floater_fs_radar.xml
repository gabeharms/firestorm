<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<floater name="radar" title="Radar">
	<filter_editor label="Filtruj ludzi" name="filter_input"/>
	<panel name="button_bar">
		<layout_stack name="bottom_bar_ls">
			<layout_panel name="view_profile_btn_lp">
				<button label="Profil" name="view_profile_btn" tool_tip="Pokaż zdjęcie, grupy i inne informacje o Rezydencie"/>
			</layout_panel>
			<layout_panel name="im_btn_lp">
				<button label="IM" name="im_btn" tool_tip="Otwórz sesję wiadomości IM"/>
			</layout_panel>
			<layout_panel name="call_btn_lp">
				<button label="Dzw." name="call_btn" tool_tip="Zadzwoń do tego Rezydenta"/>
			</layout_panel>
			<layout_panel name="share_btn_lp">
				<button label="Daj" name="share_btn" tool_tip="Udostępnij obiekt z Szafy"/>
			</layout_panel>
			<layout_panel name="tp_btn_lp">
				<button label="Teleport" name="teleport_btn" tool_tip="Zaproponuj teleport"/>
			</layout_panel>
		</layout_stack>
	</panel>
</floater>
