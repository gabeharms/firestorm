<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<floater name="floater_fs_export" title="Kopia zapasowa obiektów">
	<floater.string name="title_floater">
		Zapis [OBJECT] na twardym dysku...
	</floater.string>
	<floater.string name="title_working">
		Zapis [OBJECT] w toku - zbieranie informacji...
	</floater.string>
	<floater.string name="title_inventory">
		Zapis [OBJECT] w toku - pobieranie zawartości...
	</floater.string>
	<floater.string name="title_assets">
