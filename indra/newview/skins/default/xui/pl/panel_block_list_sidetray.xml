<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<panel name="block_list_panel">
	<layout_stack name="block_list_sidetray_main">
		<layout_panel name="back_button_container">
			<text name="title_text">
				Lista blokad
			</text>
		</layout_panel>
		<layout_panel name="block_list_blocklaypanels">
			<scroll_list name="blocked" tool_tip="Lista zablokowanych osób">
				<scroll_list.columns label="Nazwa" name="item_name"/>
				<scroll_list.columns label="Typ" name="item_type"/>
			</scroll_list>
			<button label="Zablokuj Rezydenta..." label_selected="Zablokuj Rezydenta..." name="Block resident..." tool_tip="Wybierz Rezydenta do zablokowania"/>
			<button label="Zablokuj obiekt według nazwy..." label_selected="Zablokuj obiekt według nazwy..." name="Block object by name..." tool_tip="Wybierz obiekt do zablokowania"/>
			<button label="Odblokuj" label_selected="Odblokuj" name="Unblock" tool_tip="Usuń Rezydenta lub obiekt z listy zablokowanych"/>
			<button label="Profil" label_selected="Profil" name="Profile" tool_tip="Otwiera profil zablokowanego Rezydenta"/>
		</layout_panel>
	</layout_stack>
</panel>
