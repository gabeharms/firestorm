			<combo_box.item
			 enabled="true"
			 label="Полное имя"
			 name="Complete_Name"
			 value="1" />
			<combo_box.item
			 enabled="true"
			 label="Отображаемое имя"
			 name="Display_Name"
			 value="2" />
			<combo_box.item
			 enabled="true"
			 label="Наследственное имя"
			 name="Legacy_Name"
			 value="3" />
			<combo_box.item
			 enabled="true"
			 label="Имя пользователя"
			 name="User_Name"
			 value="4" />
		</combo_box>

		<check_box
		top_pad="5"
		control_name="ShowNetStats"
		follows="left|top"
		height="16"
		initial_value="false"
		label="Show lag meter"
		left="15"
		width="270"
		tool_tip="Show lag meter in the Status Overlay"/>

		<check_box
		top_pad="5"
		follows="left|top"
		height="16"
		label="Не отсылать физику аватара серверу (old V1 client compatibility)"
