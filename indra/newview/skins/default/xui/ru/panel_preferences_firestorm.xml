			left="30"
			name="disableminzoomdist"
			width="270"
			control_name="PhoenixDisableMinZoomDist"
			tool_tip="Disable the constraint on the closest distance the camera is allowed to get to an object."/>

		<check_box top_pad="4"
			follows="left|top"
			height="16"
			initial_value="false"
			label="Разрешить двигаться камере без ограничений(не через примитивы)"
			layout="topleft"
			left="30"
			name="ignorecameraconstraints"
			width="350"
			control_name="PhoenixIgnoreSimulatorCameraConstraints"
			tool_tip="Ignores the 'push' the simulator applies to your camera to keep it out of objects."/>

		<text
			name="requires_restart_label"
			follows="left|top"
			layout="topleft"
			left_pad="10"
			top_delta="0"
			height="20"
			width="130"
			text_color="White_25">
			(требуется перезапуск)
		</text>

		<check_box
		 top_pad="12"
		 follows="left|top"
		 height="16"
		 initial_value="false"
		 label="Повернуть радар по поз. аватара"
