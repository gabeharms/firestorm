			 function="FS.ToggleSortContacts" />
		</check_box>
<!--To be removed when LL migrates beta serch to stable-->
		<text
		 type="string"
		 length="1"
		 follows="left|top"
		 height="15"
		 layout="topleft"
		 left="30"
		 name="Search_Label"
