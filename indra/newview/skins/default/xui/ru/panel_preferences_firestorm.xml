		 width="331">
			<radio_item
			 height="16"
			 label="Never (Original behaviour)"
			 layout="topleft"
			 name="radio_horizontal"
			 value="0"
			 top_pad="0"
			 width="150" />
			<radio_item
			 height="16"
			 label="Revoke on SIT"
			 layout="topleft"
			 top_pad="0"
			 name="radio_vertical"
			 value="1"
			 width="150" />
			<radio_item
			 height="16"
			 label="Revoke on STAND"
			 layout="topleft"
