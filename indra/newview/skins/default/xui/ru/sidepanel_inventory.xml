<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<panel label="Имещество" name="objects panel">
	<panel label="" name="sidepanel__inventory_panel">
		<layout_stack name="inventory_layout_stack">
			<layout_panel name="inbox_outbox_layout_panel">
				<layout_stack name="inbox_outbox_layout_stack">
					<layout_panel name="inbox_layout_panel">
						<panel label="" name="marketplace_inbox">
							<string name="InboxLabelWithArg">
								Полученные товарЫ ([NUM])
							</string>
							<string name="InboxLabelNoArg">
								Полученные товары
							</string>
							<button label="Erhaltene Artikel" name="inbox_btn"/>
							<text name="inbox_fresh_new_count">
								[NUM] нов.
							</text>
							<panel tool_tip="Перетащите элементы  чтобы управлять ими и использовать их">
								<text name="inbox_inventory_placeholder">
									Покупки с маркетплейса (Marketplace) .
								</text>
							</panel>
						</panel>
					</layout_panel>
					<layout_panel name="outbox_layout_panel">
						<panel label="" name="marketplace_outbox">
							<string name="OutboxLabelWithArg">
								Коробка дилера ([NUM])
							</string>
							<string name="OutboxLabelNoArg">
								Коробка дилера 
							</string>
							<button label="Коробка дилера " name="outbox_btn"/>
							<button label="" name="outbox_sync_btn" tool_tip="Переместить в магазин (Marketplace) "/>
							<panel>
								<panel name="outbox_inventory_placeholder_panel">
									<text name="outbox_inventory_placeholder_title">
										Загрузка...
									</text>
								</panel>
							</panel>
						</panel>
					</layout_panel>
				</layout_stack>
			</layout_panel>
		</layout_stack>
		<panel name="button_panel">
			<layout_stack name="button_panel_ls">
				<layout_panel name="info_btn_lp">
					<button label="Профиль" name="info_btn" tool_tip="Показать профиль объекта"/>
				</layout_panel>
				<layout_panel name="share_btn_lp">
					<button label="Поделиться" name="share_btn" tool_tip="Поделиться элементом инвентаря"/>
				</layout_panel>
				<layout_panel name="shop_btn_lp">
					<button label="Магазин" name="shop_btn" tool_tip="Открыть сайт с магазином"/>
					<button label="Надеть" name="wear_btn" tool_tip="Надеть выбранный костюм"/>
					<button label="Воспроизвести" name="play_btn"/>
					<button label="Телепорт" name="teleport_btn" tool_tip="Телепортироваться в выбранное место"/>
				</layout_panel>
			</layout_stack>
		</panel>
	</panel>
</panel>
