<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<context_menu
 layout="topleft"
 name="Wearing">
    <menu_item_call
     label="Править"
     layout="topleft"
     name="edit_item">
        <on_click
         function="Wearing.EditItem" />
    </menu_item_call>
    <menu_item_call
     label="Снять"
     layout="topleft"
     name="take_off">
        <on_click
         function="Wearing.TakeOff" />
    </menu_item_call>
    <menu_item_call
     label="Отделить"
     layout="topleft"
     name="detach">
        <on_click
         function="Wearing.Detach"
         parameter="detach"/>
    </menu_item_call>
    <menu_item_call
     label="Снять / Отделить"
     layout="topleft"
     name="take_off_or_detach">
        <on_click
         function="Wearing.TakeOffDetach" />
    </menu_item_call>
    <menu_item_separator
     layout="topleft"
     name="edit_outfit_separator" />
    <menu_item_call
     label="Править костюм"
     layout="topleft"
     name="edit">
        <on_click
         function="Wearing.EditOutfit" />
    </menu_item_call>
</context_menu>
