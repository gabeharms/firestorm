          <panel name="second_life_panel">

            <text name="Key:" value="UUID:"/>
            <text_editor name="user_key" value="(загрузка)"/>
            <button name="copy_uri" label="URI копировать"/>
            <text name="Имя:" value="Имя:"/>
            <text_editor name="complete_name" value="(загрузка)"/>
            <button name="copy_to_clipboard" tool_tip="Копировать в буфер обмена"/>
            <text name="status" value="Online"/>
            <text name="label" value="Рожден:"/>
            <text name="label2" value="Аккаунт:"/>
            <text name="partner_label" value="Партнер:"/>
            <text left="9" name="Groups:" value="Группы:"/>
            <group_list left_pad="3" name="group_list"/>
            <text left="9" name="Про:" value="Инфо:"/>
            <view_border left_pad="3" name="info_border"/>

            <layout_stack>
				<layout_panel name="left_buttonstack">
					<button label="Найти на карте" label_selected="Найти на карте" name="show_on_map_btn" tool_tip="Найдите пользователя на карте"/>
					<button label="Заплатить" label_selected="Заплатить" name="pay" tool_tip="Заплатить денег или поделиться инвентарем с пользователем."/>
				</layout_panel>

				<layout_panel name="middle_buttonstack">
					<button label="Отправить телепорт" label_selected="Отправить телепорт" name="teleport" tool_tip="Отправить телепорт пользователю."/>
					<button label="Личное сообщение" label_selected="Личное сообщение" name="im" tool_tip="Личное сообщение..."/>
				</layout_panel>
				
				<layout_panel name="right_buttonstack">
					<button label="Добавить друга" label_selected="Добавить друга" name="add_friend" tool_tip="Отправить запрос дружбы пользователю."/>
					<button label="Блокировать" name="block" tool_tip="Заблокировать пользователя."/>
					<button label="Разблокировать" name="unblock" tool_tip="Разблокировать пользователя."/>
				</layout_panel>
            </layout_stack>

          </panel>
</panel>

