<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<panel
 follows="all"
 height="26"
 layout="topleft"
 name="panel_voice_effect"
 width="200">
  <string name="no_voice_effect">
    Трансформация голоса выключена
  </string>
  <string name="preview_voice_effects">
    Пердпросмотр голосовой трансформации  ▶
  </string>
  <string name="get_voice_effects">
    Получить голосовую трансформацию  ▶
  </string>
  <combo_box
   enabled="false"
   follows="left|top|right"
   height="23"
   name="voice_effect"
   tool_tip="Выберите голосовую трансформацию для изменения вашего голоса"
   top_pad="0"
   width="200">
    <combo_box.item
     label="Голосовая трансформация выключена"
     name="no_voice_effect"
	 top_pad="0"
     value="0" />
    <combo_box.commit_callback
     function="Voice.CommitVoiceEffect" />
  </combo_box>
</panel>
