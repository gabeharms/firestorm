		<text name="default_grid">
			([DEFAULT])
		</text>
		<button label="Eliminar" label_selected="Eliminar" name="btn_delete"/>
		<button label="Nueva" label_selected="Nueva" name="btn_new"/>
		<!--
		<button label="Copiar" label_selected="Copiar" name="btn_copy"/>
		-->
