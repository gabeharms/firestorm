<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<panel label="Grids" name="grids_panel">
	<panel.string name="Required">
		&lt;requerido&gt;
	</panel.string>
	<panel.string name="New">
		&lt;nueva&gt;
	</panel.string>
	<panel name="layoutfix">
