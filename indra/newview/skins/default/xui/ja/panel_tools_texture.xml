<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<panel label="材質" name="Texture">

	<panel.string name="string repeats per meter">
		メートルごとに繰り返す
	</panel.string>
	<panel.string name="string repeats per face">
		面ごとに繰り返す
	</panel.string>
	<text name="color label">
		色
	</text>
	<!-- label is blank because control places it below the box -->
	<color_swatch label="" name="colorswatch" tool_tip="クリックしてカラーピッカーを開きます。" />
	<text name="color trans">
		透過度 %
	</text>
	<spinner name="ColorTrans" />
	<text name="glow label">
		グロー
	</text>
	<spinner name="glow" />
	<check_box label="明るさ全開" name="checkbox fullbright" />
	<combo_box name="combobox matmedia">
		<combo_box.item label="材質" name="Materials" value="Materials" />
		<combo_box.item label="メディア" name="Media" value="Media" />
	</combo_box>
	<combo_box name="combobox mattype">
		<combo_box.item label="テクスチャ (乱反射)" name="Texture (diffuse)" value="Texture (diffuse)" />
		<combo_box.item label="凹凸 (通常)" name="Bumpiness (normal)" value="Bumpiness (normal)" />
		<combo_box.item label="輝き (正反射)" name="Shininess (specular)" value="Shininess (specular)" />
	</combo_box>
	<texture_picker label="テクスチャ" name="texture control" tool_tip="クリックして画像を選択します。" />
	<text name="label alphamode">
		アルファモード
	</text>
	<combo_box name="combobox alphamode">
		<combo_box.item label="なし" name="None" value="None" />
		<combo_box.item label="アルファブレンド" name="Alpha blending" value="Alpha blending" />
		<combo_box.item label="アルファマスク" name="Alpha masking" value="Alpha masking" />
		<combo_box.item label="放射マスク" name="Emissive mask" value="Emissive mask" />
	</combo_box>
	<text name="label maskcutoff">
		マスクカットオフ
	</text>
	<spinner name="maskcutoff" />
	<texture_picker label="テクスチャ" name="bumpytexture control" tool_tip="クリックして画像を選択します。" />
	<text name="label bumpiness">
		凹凸
	</text>
	<combo_box name="combobox bumpiness">
		<combo_box.item label="なし" name="None" value="None" />
		<combo_box.item label="明るさ" name="Brightness" value="Brightness" />
		<combo_box.item label="暗さ" name="Darkness" value="Darkness" />
        <combo_box.item label="木目調" name="woodgrain" value="woodgrain" />
		<combo_box.item label="木の皮" name="bark" value="bark" />
		<combo_box.item label="レンガ" name="bricks" value="bricks" />
		<combo_box.item label="チェッカー" name="checker" value="checker" />
		<combo_box.item label="コンクリート" name="concrete" value="concrete" />
		<combo_box.item label="クラスティ・タイル" name="crustytile" value="crustytile" />
		<combo_box.item label="カットストーン" name="cutstone" value="cutstone" />
		<combo_box.item label="ディスク" name="discs" value="discs" />
		<combo_box.item label="砂利" name="gravel" value="gravel" />
		<combo_box.item label="ペトリ" name="petridish" value="petridish" />
		<combo_box.item label="羽根板" name="siding" value="siding" />
		<combo_box.item label="ストーン・タイル" name="stonetile" value="stonetile" />
		<combo_box.item label="しっくい" name="stucco" value="stucco" />
		<combo_box.item label="吸い込み" name="suction" value="suction" />
		<combo_box.item label="織り目" name="weave" value="weave" />
		<!--
			NORSPEC-182, ensure item doesn't show up in menu until it should
				<combo_box.item
				label="Use texture"
				name="Use texture"
				value="Use texture" />
		-->
	</combo_box>
	<texture_picker label="テクスチャ" name="shinytexture control" tool_tip="クリックして画像を選択します。" />
	<text name="label shininess">
		輝き
	</text>
	<combo_box name="combobox shininess">
		<combo_box.item label="なし" name="None" value="None" />
		<combo_box.item label="低" name="Low" value="Low" />
		<combo_box.item label="中" name="Medium" value="Medium" />
		<combo_box.item label="高" name="High" value="High" />
		<!--
			NORSPEC-182, ensure item doesn't show up in menu until it should
			<combo_box.item
			label="Use texture"
			name="Use texture"
			value="Use texture" />
		-->
	</combo_box>
	<text name="label glossiness">
		光沢
	</text>
	<spinner name="glossiness" />
	<text name="label environment">
		環境
	</text>
	<spinner name="environment" />
	<text name="label shinycolor">
		色
	</text>
	<!-- label is blank because control places it below the box -->
	<color_swatch label="" name="shinycolorswatch" tool_tip="クリックしてカラーピッカーを開きます。" />
	<text name="media_info">
		メディアを選択した場合、ここにURLが表示されます。
	</text>
	<button name="add_media" tool_tip="メディアを追加します。" label="選択..." />
	<button name="delete_media" tool_tip="このメディアテクスチャを削除します。" label="削除" />
	<button label="揃える" name="button align" tool_tip="メディアテクスチャを一列に揃えます。（最初に読み込む必要があります。）" />
	<text name="tex gen">
		マッピング
	</text>
	<combo_box name="combobox texgen">
		<combo_box.item label="Default" name="デフォルト" value="Default" />
		<combo_box.item label="Planar" name="平面" value="Planar" />
	</combo_box>
	<spinner label="水平（U）" name="TexScaleU" />
	<spinner label="垂直（V）" name="TexScaleV" />
	<spinner label="反復 / メーター" name="rptctrl" />
	<spinner label="回転˚" name="TexRot" />
	<spinner label="水平のズレ（U）" name="TexOffsetU" />
	<spinner label="垂直のズレ（V）" name="TexOffsetV" />
	<spinner label="水平（U）" name="bumpyScaleU" />
	<spinner label="垂直（V）" name="bumpyScaleV" />
	<spinner label="回転˚" name="bumpyRot" />
	<spinner label="水平のズレ（U）" name="bumpyOffsetU" />
	<spinner label="垂直のズレ（V）" name="bumpyOffsetV" />
	<spinner label="水平（U）" name="shinyScaleU" />
	<spinner label="垂直（V）" name="shinyScaleV" />
	<spinner label="回転˚" name="shinyRot" />
	<spinner label="水平のズレ（U）" name="shinyOffsetU" />
	<spinner label="垂直のズレ（V）" name="shinyOffsetV" />
	<check_box label="平面を揃える" name="checkbox planar align" tool_tip="選択面全てのテクスチャを、最後に選択された面に揃えます。 平面テクスチャのマッピングが必要です。" />
	<web_browser name="title_media" />

</panel>
