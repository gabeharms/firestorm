<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<panel name="panel_profile_classified" label="広告">

    <loading_indicator name="progress_indicator" />
	<string name="no_classifieds" value="広告掲載なし" />
    <flat_list_view name="classifieds_list" />

	<panel name="edit_panel">

	<!-- Replaces LLs button panel as they had not put buttons in -->

		<button name="new_btn" tool_tip="今いる場所の広告を作成します。" />
		<icon name="dummy_icon" />
		<dnd_button name="trash_btn" />
	</panel>

	<panel name="buttons_cucks">
		<layout_stack name="buttons_cucks_ls">
			<layout_panel name="info_btn_lp">
				<button label="情報" name="info_btn" tool_tip="選択した広告の情報を表示します。" />
			</layout_panel>
			<layout_panel name="teleport_btn_lp">
                <button label="テレポート" name="teleport_btn" tool_tip="該当の地域にテレポートします。" />
			</layout_panel>
			<layout_panel name="show_on_map_btn_lp">
                <button label="地図" name="show_on_map_btn" tool_tip="該当の地域を世界地図に表示します。" />
			</layout_panel>
		</layout_stack>
	</panel>
    
</panel>

