	<layout_stack name="buttonstack">
		<layout_panel name="left_buttonstack">
			<button label="Zeige auf Karte" label_selected="Zeige auf Karte" name="show_on_map_btn" tool_tip="Zeige Position des Einwohners auf der Karte."/>
