<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<floater
    legacy_header_height="18"
    can_resize="false"
    can_dock="true"
    can_close="false"
    height="200"
    width="260"
    layout="topleft"
    name="quickprefs"
    help_topic="quickprefs"
    save_rect="true"
    save_visibility="true"
    single_instance="true"
    save_dock_state="true"
    title="Quick Preferences">
<panel
    name="quickprefs"
    left="0"
    top="16"
    width="260"
    height="200"
    follows="all"
    layout="topleft">
        <text
            type="string"
            length="1"
            follows="left|top"
            height="10"
            layout="topleft"
            left="5"
            name="Maximum bandwidth"
            mouse_opaque="false"
            top="10"
            width="90">
            Max Bandwidth:
        </text>
        <slider
            can_edit_text="true"
            control_name="ThrottleBandwidthKBPS"
            decimal_digits="0"
            follows="left|top"
            height="15"
            increment="100"
            initial_value="500"
            layout="topleft"
            left_delta="100"
            max_val="10000"
            min_val="100"
            name="max_bandwidth"
            top_delta="-2"
            width="150" />
        <text
            type="string"
            length="1"
            follows="left|top"
            height="10"
            layout="topleft"
            left="5"
            name="Draw Distance"
            mouse_opaque="false"
            top_delta="20"
            width="90">
            Draw Distance:
        </text>
        <slider
            can_edit_text="true"
            control_name="RenderFarClip"
            decimal_digits="0"
            follows="left|top"
            height="15"
            increment="8"
            initial_value="160"
            layout="topleft"
            left_delta="93"
            max_val="1024"
            min_val="32"
            name="DrawDistance"
            top_delta="-2"
            width="150"
            label_width="3"/>
        <text
            type="string"
            length="1"
            follows="left|top"
            height="10"
            layout="topleft"
            left="5"
            name="Particles"
            mouse_opaque="false"
            top_delta="20"
            width="90">
            Max Particles:
        </text>
        <slider
            can_edit_text="true"
            control_name="RenderMaxPartCount"
            decimal_digits="0"
            follows="left|top"
            height="15"
            increment="256"
            initial_value="4096"
            layout="topleft"
            left_delta="93"
            max_val="8192"
            min_val="0"
            name="RenderMaxPartCount"
            top_delta="-2"
            width="150"
            label_width="3"/>
        <text
            type="string"
            length="1"
            follows="left|top"
            height="10"
            layout="topleft"
            left="5"
            name="Max Node"
            mouse_opaque="false"
            top_delta="20"
            width="90">
            Max Node size:
        </text>
        <slider
            can_edit_text="true"
            control_name="RenderMaxNodeSize"
            decimal_digits="0"
            follows="left|top"
            height="15"
            increment="64"
            initial_value="8192"
            layout="topleft"
            left_delta="100"
            max_val="16382"
            min_val="64"
            name="RenderMaxNodeSize"
            top_delta="-2"
            width="150"
            label_width="0"/>
        <text
            type="string"
            length="1"
            follows="left|top"
            height="10"
            layout="topleft"
            left="5"
            name="LOD Factor"
            mouse_opaque="false"
            top_delta="20"
            width="90">
            LOD Factor:
        </text>
        <slider
            can_edit_text="true"
            control_name="RenderVolumeLODFactor"
            decimal_digits="1"
            follows="left|top"
            height="15"
            increment="0.1"
            initial_value="2"
            layout="topleft"
            left_delta="82"
            max_val="8"
            min_val="0"
            name="RenderVolumeLODFactor"
            top_delta="-2"
            width="150"
            label_width="14"/>
        <text
            type="string"
            length="1"
            follows="left|top"
            height="10"
            layout="topleft"
            left="5"
            name="Max Avatars"
            mouse_opaque="false"
            top_delta="20"
            width="90">
            Max Avatars:
        </text>
        <slider
            can_edit_text="true"
            control_name="RenderAvatarMaxVisible"
            decimal_digits="0"
            follows="left|top"
            height="15"
            increment="1"
            initial_value="12"
            layout="topleft"
            left_delta="79"
            max_val="50"
            min_val="1"
            name="RenderAvatarMaxVisible"
            top_delta="-2"
            width="150"
            label_width="17"/>
        <text
            type="string"
            length="1"
            follows="left|top"
            height="10"
            layout="topleft"
            left="5"
            name="WL Sky"
            mouse_opaque="false"
            top_delta="40"
            width="90">
            WindLight Sky:
        </text>
        <combo_box
            height="20"
            layout="topleft"
            left_delta="100"
            name="WLPresetsCombo"
            top_delta="-2"
            width="150" />
        <text
            type="string"
            length="1"
            follows="left|top"
            height="10"
            layout="topleft"
            left="5"
            name="WL Water"
            mouse_opaque="false"
            top_delta="25"
            width="90">
            WindLight Water:
        </text>
        <combo_box
            height="20"
            layout="topleft"
            left_delta="100"
            name="WaterPresetsCombo"
            top_delta="-2"
            width="150" />
</panel>
</floater>
