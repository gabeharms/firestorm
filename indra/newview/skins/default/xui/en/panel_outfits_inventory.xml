<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<panel name="Outfits"
 follows="all"
border="false">
    <accordion
     single_expansion="true"
      follows="top|left|bottom"
      height="460"
      layout="topleft"
     left="0"
     name="outfits_accordion"
     top="0"
     width="333">
     <accordion_tab
         layout="topleft"
         name="tab_cof"
         title="Current Outfit">
	 <inventory_panel
	 	 allow_multi_select="true"
		 border="false"
		 bottom="0"
	         follows="all"
		 height="416"
		 left="0"
		 mouse_opaque="true"
	         name="cof_accordionpanel"
		 width="333"
		 start_folder="Current Outfit" />
        </accordion_tab>
        <accordion_tab
         layout="topleft"
         name="tab_outfits"
         title="My Outfits">
	 <inventory_panel
	 	 allow_multi_select="true"
		 border="false"
		 bottom="0"
	     follows="all"
		 height="415"
		 left="0"
		 mouse_opaque="true"
	     name="outfitslist_accordionpanel"
		 width="333"
		 start_folder="My Outfits" />
        </accordion_tab>
	</accordion>
