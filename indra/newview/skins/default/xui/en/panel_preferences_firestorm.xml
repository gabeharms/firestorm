<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<panel
 border="true"
 follows="all"
 height="440"
 label="Firestorm"
 layout="topleft"
 left="1"
 name="firestorm"
 top="1"
 width="535">

<tab_container
 label="Firestorm Prefs"
 layout="topleft"
 follows="all"
 top="5"
 bottom="-10"
 left="1"
 right="-1"
 name="tabs"
 tab_min_width="50"
 tab_position="top" >

	<!--General-->
	<panel
	 top_pad="5"
	 bottom="-1"
	 left="1"
	 right="-1"
	 follows="all"
	 label="General"
	 name="tab-general" >
		<check_box
		 control_name="RestrainedLove"
		 layout="topleft"
		 follows="left|top"
		 top_pad="10"
		 left="20"
		 width="270"
		 height="20"
		 name="checkMiscRLVa"
		 label="Allow Remote Scripted Viewer Controls  (RLVa)" >
			<check_box.commit_callback
			 function="RLV.ToggleEnabled" />
			<check_box.enabled_callback
			 function="RLV.CheckEnabled" />
		</check_box>
		<text
		 layout="topleft"
		 follows="left|top"
		 left_pad="3"
		 height="20"
		 width="150"
		 name="textMiscRLVaRestart"
		 text_color="White_25">
		(requires restart)
		</text>
		<text
		 follows="left|top"
		 layout="topleft"
		 left="20"
		 height="20"
		 name="tab_direction_label"
		 width="150"
		 top_pad="5">
		  Chat Tab Mode Orientation:
		</text>
		<text
		 name="requires_restart_label"
		 follows="left|top"
		 layout="topleft"
		 left_pad="5"
		 height="20"
		 width="130"
		 text_color="White_25">
		  (requires restart)
		</text>
		<radio_group
		 height="16"
		 layout="topleft"
		 left="40"
		 control_name="ChatTabDirection"
		 name="chat_tab_direction"
		 top_pad="0"
		 tool_tip="Change flow of the chat/IM tabs between horizontal and vertical when consolidated tab mode is active. (Requires restart)"
		 width="331">
			<radio_item
			 height="16"
			 label="Horizontal"
			 layout="topleft"
			 left="0"
			 name="radio_horizontal"
			 value="0"
			 top="0"
			 width="150" />
			<radio_item
			 height="16"
			 label="Vertical"
			 layout="topleft"
			 left_delta="160"
			 name="radio_vertical"
			 value="1"
			 width="150" />
		</radio_group>
		<check_box
		 top_pad="5"
		 follows="left|top"
		 height="16"
		 label="Disable Teleport Screens"
		 left="15"
		 name="tp_screen_toggle"
		 width="270"
		 control_name="PhoenixDisableTeleportScreens"
		 tool_tip="Disables the black teleport screen during all teleports."/>
		<check_box
		 top_pad="5"
		 follows="left|top"
		 height="16"
		 label="Disable Login Progress Screen"
		 left_delta="0"
		 name="login_screen_toggle"
		 width="270"
		 control_name="PhoenixDisableLoginScreens"
		 tool_tip="Disables the black login progress Screen"/>
		<check_box
		 top_pad="5"
		 follows="left|top"
		 height="16"
		 label="Disable Logout Progress Screen"
		 left_delta="0"
		 name="logout_screen_toggle"
		 width="270"
		 control_name="PhoenixDisableLogoutScreens"
		 tool_tip="Disables the black Logout progress screen."/>
		<check_box
		 top_pad="5"
		 follows="left|top"
		 height="16"
		 label="Always rez objects under the land group if possible"
		 left="15"
		 name="grouplandrez"
		 width="270"
		 control_name="RezUnderLandGroup"
		 tool_tip="Attempts to rez objects under the land group if you're in the land group regardless of which current group tag you're wearing."/>
		<check_box top_pad="5"
		 follows="left|top"
		 height="16"
		 initial_value="false"
		 label="Filter duplicate Landmarks on World Map"
		 left="15"
		 name="WorldmapFilterDuplicateLandmarks"
		 width="270"
		 control_name="WorldmapFilterDuplicateLandmarks"
		 tool_tip="Filters duplicate Landmark entries in the Landmark list on the World Map."/>
		<check_box top_pad="5"
		 follows="left|top"
		 height="16"
		 label="Turn avatar around when walking backwards"
		 left="15"
		 name="backwardsturnavatar"
		 width="270"
		 control_name="TurnAroundWhenWalkingBackwards"
		 tool_tip="Turns your avatar around to face the camera when you are walking backwards."/>
		<check_box top_pad="5"
		 follows="left|top"
		 height="16"
		 label="Show look at target"
		 left="15"
		 name="showlookat"
		 width="270"
		 control_name="DebugLookAt"
		 tool_tip="Enable crosshairs marking where people are looking."/>
		<text
		 type="string"
		 length="1"
		 follows="left|top"
		 height="15"
		 layout="topleft"
		 left="30"
		 name="lookatshownames"
		 top_pad="5"
		 width="200">
			Show names for look at target:
		</text>
		<combo_box
		 control_name="DebugLookAtShowNames"
		 follows="left|top"
		 height="23"
		 layout="topleft"
		 left_pad="7"
		 max_chars="135"
		 name="lookatshownames_combobox"
		 width="150">
			<combo_box.item
			 enabled="true"
			 label="None"
			 name="None"
			 value="0" />
			<combo_box.item
			 enabled="true"
			 label="Complete name"
			 name="Complete_Name"
			 value="1" />
			<combo_box.item
			 enabled="true"
			 label="Display name"
			 name="Display_Name"
			 value="2" />
			<combo_box.item
			 enabled="true"
			 label="Legacy name"
			 name="Legacy_Name"
			 value="3" />
			<combo_box.item
			 enabled="true"
			 label="Username"
			 name="User_Name"
			 value="4" />
		</combo_box>

		<check_box
		top_pad="5"
		control_name="ShowNetStats"
		follows="left|top"
		height="16"
		initial_value="false"
		label="Show lag meter"
		left="15"
		width="270"
		tool_tip="Show lag meter in the Status Overlay"/>

		<check_box
		top_pad="5"
		follows="left|top"
		height="16"
		label="Don't send avatar physics to server (old V1 client compatibility)"
		left="15"
		name="dontsendavphysics"
		width="270"
		control_name="FSDontSendAvPhysicsParms"
		tool_tip="Disables sending your avatar physics information to the Second Life server, for compatibility with version 1 viewers"/>

		<check_box
		top_pad="5"
		follows="left|top"
		height="16"
		label="Sort contact lists by username even when display names are shown"
		left="15"
		name="sortcontactsbyun"
		width="270"
		control_name="FSSortContactsByUserName"
		tool_tip="If this is checked, all contact lists are always sorted by username">
			<check_box.commit_callback
			 function="FS.ToggleSortContacts" />
		</check_box>

		<check_box
		 top_pad="5"
		 follows="left|top"
		 height="16"
		 label="Render blocked avatars as particle cloud"
		 left="15"
		 name="ShowMutedAvatarsAsCloud"
		 width="270"
		 control_name="ShowMutedAvatarsAsCloud"
		 tool_tip="Blocked avatars will be rendered as a particle cloud in special color.">
			<check_box.commit_callback
			 function="FS.ToggleSortContacts" />
		</check_box>
<!--To be removed when LL migrates beta serch to stable-->
		<text
		 type="string"
		 length="1"
		 follows="left|top"
		 height="15"
		 layout="topleft"
		 left="30"
		 name="Search_Label"
		 top_delta="25"
		 width="130"
		 tool_tip="While the beta URL often provides more accurate search results, it may not always be available.">
		  Default Search URL:
		</text>

    <text
     name="requires_restart_label"
     follows="left|top"
     layout="topleft"
     left_delta ="115"
     height="20"
     width="90"
     text_color="White_25">
      (requires restart)
     </text>

		<combo_box
		 control_name="SearchURL"
		 follows="left|top"
		 height="23"
		 layout="topleft"
		 left_delta="95"
		 max_chars="135"
		 name="SearchURL"
		 width="150">
			<combo_box.item
			 enabled="true"
			 label="Standard SL search"
			 name="Standard V2 search"
			 value="http://search.secondlife.com/viewer/[CATEGORY]/?q=[QUERY]&amp;p=[AUTH_TOKEN]&amp;r=[MATURITY]&amp;lang=[LANGUAGE]&amp;sid=[SESSION_ID]" />
			<combo_box.item
			 enabled="true"
			 label="Beta SL Search"
			 name="Beta V2 Search"
			 value="http://search-beta.secondlife.com/viewer/[CATEGORY]/?q=[QUERY]&amp;p=[AUTH_TOKEN]&amp;r=[MATURITY]&amp;lang=[LANGUAGE]&amp;g=[GODLIKE]&amp;sid=[SESSION_ID]&amp;rid=[REGION_ID]&amp;pid=[PARCEL_ID]&amp;channel=[CHANNEL]&amp;version=[VERSION]&amp;major=[VERSION_MAJOR]&amp;minor=[VERSION_MINOR]&amp;patch=[VERSION_PATCH]&amp;build=[VERSION_BUILD]" />
		</combo_box>
		<check_box
		top_pad="15"
		control_name="UseLSLBridge"
		follows="left|top"
		height="16"
		initial_value="true"
		label="Enable LSL-Client Bridge"
		left="15"
		width="270"
		tool_tip="Allows the Viewer to use a scripted attachment (Bridge) to extend its features"/>
	</panel>


	<!--Protection-->
	<panel
	 top_pad="5"
	 bottom="-1"
	 left="1"
	 right="-1"
	 follows="all"
	 label="Protection"
	 name="ProtectionTab">
		<check_box
		 top_pad="12"
		 left="10"
		 label="Block left-click sitting on objects"
		 control_name="PhoenixBlockClickSit"
		 follows="left|top"
		 height="16"
		 name="PhoenixBlockClickSit"
		 width="256" />
		<check_box
		 top_pad="8"
		 label="Allow scripts to show map UI (llMapDestination)"
		 control_name="ScriptsCanShowUI"
		 follows="left|top"
		 height="16"
		 name="ScriptsCanShowUI"
		 width="256" />

		<text
		 top_pad="10"
		 follows="left|top"
		 height="12"
		 name="TextText"
		 width="200">
		 Revoke Permissions:
		 </text>
		<radio_group
		 top_pad="5"
		 left="20"
		 height="62"
		 layout="topleft"
		 control_name="PhoenixRevokePerms"
		 name="PhoenixRevokePerms"
		 tool_tip="PhoenixRevokePerms"
		 width="331">
			<radio_item
			 height="16"
			 label="Never (Original behaviour)"
			 layout="topleft"
			 name="radio_horizontal"
			 value="0"
			 top_pad="0"
			 width="150" />
			<radio_item
			 height="16"
			 label="Revoke on SIT"
			 layout="topleft"
			 top_pad="0"
			 name="radio_vertical"
			 value="1"
			 width="150" />
			<radio_item
			 height="16"
			 label="Revoke on STAND"
			 layout="topleft"
			 top_pad="0"
			 name="radio_vertical"
			 value="2"
			 width="150" />
			<radio_item
			 height="16"
			 label="Revoke on SIT and STAND"
			 layout="topleft"
			 top_pad="0"
			 name="radio_vertical"
			 value="3"
			 width="150" />
		</radio_group>
		
		<text
		 left="5"
		 top_pad="10"
		 follows="left|top"
		 height="12"
		 name="SurfaceAreaThreshholdLabel"
		 width="300">
		 Texture Lag Protection (may break some objects):
		 </text>
		 <check_box
		 top_pad="10"
		 left="20"
		 label="Automatically hide large objects with high texture area. Treshhold: "
		 control_name="RenderVolumeSAProtection"
		 follows="left|top"
		 height="16"
		 name="RenderVolumeSAProtection"
		 width="380" />
		 	<spinner
			 top_delta="-5"
			 control_name="RenderVolumeSAFrameMax"
			 decimal_digits="0"
			 layout="topleft"
			 follows="left|top"
			 height="18"
			 left_pad="5"
			 increment="1"
			 initial_val="5000"
			 max_val="2000"
			 min_val="500"
			 name="RenderVolumeSAFrameMax"
			 width="90" />
		
		
	</panel>


	<!--View-->
	<panel
	 top_pad="5"
	 bottom="-1"
	 left="1"
	 right="-1"
	 follows="all"
	 label="View"
	 name="firestorm_view" >

		<text
			top_pad="10"
			type="string"
			length="1"
			follows="left|top"
			height="15"
			layout="topleft"
			left="15"
			name="camera_options_label"
			width="200">
			Camera options:
		</text>

		<check_box top_pad="4"
			follows="left|top"
			height="16"
			initial_value="false"
			label="Clicking your avatar keeps camera position"
			layout="topleft"
			left="30"
			name="backwardsturnavatar"
			width="270"
			control_name="ClickOnAvatarKeepsCamera"
			tool_tip="Normally, clicking on your avatar resets the camera position. This option removes this behavior."/>

		<check_box top_pad="4"
			follows="left|top"
			height="16"
			initial_value="false"
			label="Disable minimum camera zoom distance"
			layout="topleft"
			left="30"
			name="disableminzoomdist"
			width="270"
			control_name="PhoenixDisableMinZoomDist"
			tool_tip="Disable the constraint on the closest distance the camera is allowed to get to an object."/>

		<check_box top_pad="4"
			follows="left|top"
			height="16"
			initial_value="false"
			label="Allow the camera to move without constraints through prims"
			layout="topleft"
			left="30"
			name="ignorecameraconstraints"
			width="350"
			control_name="PhoenixIgnoreSimulatorCameraConstraints"
			tool_tip="Ignores the 'push' the simulator applies to your camera to keep it out of objects."/>

		<text
			name="requires_restart_label"
			follows="left|top"
			layout="topleft"
			left_pad="10"
			top_delta="0"
			height="20"
			width="130"
			text_color="White_25">
			(requires restart)
		</text>

		<check_box
		 top_pad="12"
		 follows="left|top"
		 height="16"
		 initial_value="false"
		 label="Enable Minimap rotation"
		 layout="topleft"
		 left="15"
		 name="minimaprotate"
		 width="350"
		 control_name="MiniMapRotate"
		 tool_tip="Rotate miniature world map to avatar direction."/>

		<check_box
		 control_name="MiniMapChatRing"
		 name="MiniMapChatRing"
		 label="Display a ring on the Minimap indicating local chat range"
		 layout="topleft"
		 top_pad="0"
		 height="20"
		 width="350" />

		<check_box
		 control_name="FSAdvancedTooltips"
		 name="FSAdvancedTooltips"
		 label="Show extended information in hovertips (classic Phoenix style)"
		 layout="topleft"
		 top_pad="0"
		 height="20"
		 width="350" />
		 
		<!-- LGG Color Beams -->
        <text font="SansSerifSmall" name="BeamPrefs" follows="left|top" left="15" bottom_delta="25">
			Selection beam particle effects
		</text>
		<view_border bevel_style="none" border_thickness="1" top_delta="15" follows="top|left" height="0" left="15" name="EmDivisor" width="490"/>

		<button
		 name="BeamColor_delete"
		 label="Delete"
		 halign="center"
		 tool_tip="Remove this preset"
		 left="15"
		 top_delta="10"
		 width="55"
		 height="20"
		 font="SansSerifSmall"
		 follows="left|top" />

        <combo_box
		 allow_text_entry="false"
		 top_delta="0"
		 left_delta="60"
		 follows="left|top"
		 height="20"	
		 max_chars="20"
		 mouse_opaque="true"
		 name="BeamColor_combo"
		 width="170"
		 control_name="PhoenixBeamColorFile"
		 tool_tip="Select the color preset for the selection beam"/>
		
        <button
		 name="BeamColor_new"
		 label="Create New"
		 tool_tip="Create a new color preset"
		 left="15"
		 bottom_delta="25"
		 width="75"
		 height="20"
		 font="SansSerifSmall"
		 follows="left|top"/>
		 
        <button
		 name="BeamColor_refresh"
		 label="Refresh List"
		 tool_tip="Refresh the preset list"
		 left="15"
		 bottom_delta="25"
		 width="75"
		 height="20"
		 font="SansSerifSmall"
		 follows="left|top" />
		
        <button
		 left="100"
		 top_delta="-25"
		 follows="left|top"
		 width="130"
		 height="80"
		 name="PhoenixBeamPrev_rainbow"
		 label=""
		 scale_image="true"
		 image_selected="beam_rainbow.png"
		 image_unselected="beam_rainbow.png"
		 image_hover_selected="beam_rainbow.png"
		 image_hover_unselected="beam_rainbow.png" />

        <slider
		 bottom_delta="25"
		 left="15"
		 name="PhoenixMaxBeamsPerSecond"
		 control_name="PhoenixMaxBeamsPerSecond"
		 decimal_digits="0"
		 enabled="true"
		 follows="left|top"
		 height="18"
		 increment="1"
		 initial_val="4"
		 label="Beam updates/sec:"
		 label_width="100"
		 max_val="200"
		 min_val="4"
		 mouse_opaque="true"
		 show_text="true"
		 width="250"
		 auto_resize="false"
		 tool_tip="How many beam updates to send in a second. Default 40" />

        <check_box
		 bottom_delta="18"
		 enabled="true"
		 follows="left|top"
		 font="SansSerifSmall"
		 height="16"
		 initial_value="true"
		 label="Enable selection beam"
		 left="15"
		 mouse_opaque="true"
		 name="SLPartBeam"
		 radio_style="false"
		 width="250"
		 control_name="ShowSelectionBeam" />

        <check_box
		 bottom_delta="18"
		 enabled="true"
		 follows="left|top"
		 font="SansSerifSmall"
		 height="16"
		 initial_value="true"
		 label="SL Particle Chat"
		 left="15"
		 mouse_opaque="true"
		 name="PhoenixParticleChat"
		 radio_style="false"
		 width="250"
		 control_name="PhoenixParticleChat" />

		<view_border bevel_style="none" border_thickness="1" top_delta="-160" follows="top|left" height="180" left="260" name="EmDivisor2" width="0"/>
 
		<combo_box
		 allow_text_entry="false"
		 bottom_delta="-150"
		 left="275"
		 follows="left|top"
		 height="20"
		 max_chars="20"
		 mouse_opaque="true"
		 name="PhoenixBeamShape_combo"
		 width="170"
		 control_name="PhoenixBeamShape"
		 tool_tip="Select the shape for the particle beam" />

		<button
		 name="delete_beam"
		 label="Delete"
		 halign="center"
		 tool_tip="Remove this beam"
		 left_delta="175"
		 bottom_delta="0"
		 width="55"
		 height="20"
		 font="SansSerifSmall"
		 follows="left|top" />

		<button
		 name="custom_beam_btn"
		 label="Create New"
		 tool_tip="Customize the beam shape"
		 left_delta="-20"
		 bottom_delta="25"
		 width="75"
		 height="20"
		 font="SansSerifSmall"
		 follows="left|top"/>

        <button
		 name="refresh_beams"
		 label="Refresh List"
		 tool_tip="Refresh the beam list"
		 left_delta="0"
		 bottom_delta="25"
		 width="75"
		 height="20"
		 font="SansSerifSmall" 
		 follows="left|top" />

         <button
		 left="290"
		 top_delta="-25"
		 follows="left|top"
		 width="130"
		 height="80"
		 name="PhoenixBeamPrev_Phoenix"
		 label=""
		 scale_image="true"
		 image_selected="beam_phoenix.png"
		 image_unselected="beam_phoenix.png"
		 image_hover_selected="beam_phoenix.png"
		 image_hover_unselected="beam_phoenix.png" />

        <slider
		 bottom_delta="25"
		 left_delta="-15"
		 name="PhoenixBeamShapeScale"
		 control_name="PhoenixBeamShapeScale"
		 decimal_digits="1"
		 enabled="true"
		 follows="left|top"
		 height="18"
		 increment="0.1"
		 initial_val="1"
		 label="Scale:"
		 label_width="35"
		 max_val="4"
		 min_val="0.1"
		 mouse_opaque="true"
		 show_text="true"
		 width="150"
		 auto_resize="false"
		 tool_tip="Change the scale of the shape used for the Phoenix Shaped Beam" />

	</panel>

	<!--Chat-->
	<panel
	 top_pad="5"
	 bottom="-1"
	 left="1"
	 right="-1"
	 follows="all"
	 label="Chat"
	 name="firestorm_chat" >

		<tab_container
		 label="Firestorm Prefs"
		 top_pad="5"
		 bottom="-1"
		 left="1"
		 right="-1"
		 follows="all"
		 name="tabs"
		 tab_min_width="50"
		 tab_position="top" >

		<!--Chat: General-->
		<panel
		 border="true"
		 top_pad="1"
		 bottom="-1"
		 left="1"
		 right="-1"
		 follows="all"
		 label="General"
		 name="ChatGeneral" >

			<text
			 name="chat_and_in_enhancements"
			 follows="left|top"
			 layout="topleft"
			 top_pad="5"
			 left="24"
			 height="10"
			 width="180">
			  Chat and IM enhancements:
			</text>
			<check_box
			 control_name="AllowMUpose"
			 name="AllowMUpose"
			 label="&quot;:&quot; as a synonym for &quot;/me&quot;"
			 layout="topleft"
			 top_pad="0"
			 left="30"
			 height="20"
			 width="400" />
			<check_box
			 control_name="EmotesUseItalic"
			 name="EmotesUseItalic"
			 label="Emotes use italic font"
			 layout="topleft"
			 top_pad="0"
			 height="18"
			 width="400" />
			<check_box
			 control_name="AutoCloseOOC"
			 name="AutoCloseOOC"
			 label="Auto-close ((OOC)) parentheses"
			 layout="topleft"
			 top_pad="0"
			 height="18"
			 width="400" />
			<check_box
			 control_name="OnlineOfflinetoNearbyChat"
			 name="OnlineOfflinetoNearbyChat"
			 label="Friend on/off-line notices to Nearby Chat"
			 layout="topleft"
			 top_pad="0"
			 height="18"
			 width="400" />
			<check_box
			 control_name="ShowStreamMetadata"
			 name="ShowStreamMetadata"
			 label="Show stream title in local chat"
			 layout="topleft"
			 top_pad="0"
			 height="18"
			 width="400" />
			<check_box
			 control_name="StreamMetadataAnnounceToChat"
			 name="StreamMetadataAnnounceToChat"
			 label="Announce stream title to chat channel:"
			 layout="topleft"
			 top_pad="0"
			 height="18"
			 width="240" />
			<spinner
			 top_delta="-2"
			 control_name="StreamMetadataAnnounceChannel"
			 decimal_digits="0"
			 layout="topleft"
			 follows="left|top"
			 height="18"
			 left_pad="5"
			 increment="1"
			 initial_val="362394"
			 max_val="2147483647"
			 min_val="1"
			 name="StreamMetadataAnnounceChannel"
			 width="90" />
			<check_box
			 layout="topleft"
			 follows="left|top"
			 top_pad="5"
			 left="20"
			 width="250"
			 height="20"
			 control_name="PhoenixNearbyChatbar"
			 label="Add a chatbar in the Nearby Chat window"/>
			<text
			 top_delta="2"
			 name="requires_restart_label"
			 follows="left|top"
