		 max_val="256"
		 min_val="-256"
		 name="Z pos"
		 width="120"
		 control_name="PhoenixBuildPrefs_PivotZ"/>
		<check_box
		 top_pad="10"
		 left_delta="-5"
		 enabled="true"
		 follows="left|top"
		 font="SansSerifSmall"
		 height="16"
		 label="Values are percent"
		 mouse_opaque="true"
		 tool_tip="Default settings are Percentages and every axis set at 50"
		 name="EmPivotPercToggle"
		 radio_style="false"
		 width="100"
		 control_name="PhoenixBuildPrefs_PivotIsPercent"/>
		 
		<check_box
		 top_pad="20"
		 follows="left|top"
		 height="16"
		 label="Enable highlighting of selected prims"
		 tool_tip="When unchecked, the yellow/blue outline effects are not rendered, improving performance when large numbers of prims are selected."
		 name="EmBuildPrefsRenderHighlight_toggle"
		 control_name="RenderHighlightSelections"/>

		<check_box
		 follows="left|top"
		 height="16"
		 label="Use old &quot;Set Object For Sale&quot; behavior"
