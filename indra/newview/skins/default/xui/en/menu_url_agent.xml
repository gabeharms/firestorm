<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<context_menu
 layout="topleft"
 name="Url Popup">
    <menu_item_call
     label="Show Resident Profile"
     layout="topleft"
     name="show_agent">
        <!-- FS:Ansariel: FS-7263 - Make sure the agent profile is opened instead of the inspect panel
        <menu_item_call.on_click
         function="Url.Execute" /> -->
        <menu_item_call.on_click
         function="Url.ShowProfile" />
    </menu_item_call>
    <menu_item_call
     label="Zoom at Resident"
     layout="topleft"
     name="zoom_agent">
        <menu_item_call.on_click
         function="FS.ZoomIn" />
    </menu_item_call>
    <menu_item_call
     label="Teleport to Resident"
     layout="topleft"
     name="teleportto_agent">
        <menu_item_call.on_click
         function="FS.TeleportToTarget" />
    </menu_item_call>
    <menu_item_call
     label="Offer Teleport"
     layout="topleft"
     name="offer_teleport">
        <menu_item_call.on_click
         function="FS.OfferTeleport" />
    </menu_item_call>
    <menu_item_call
     label="Track Resident"
     layout="topleft"
     name="track_agent">
        <menu_item_call.on_click
         function="FS.TrackAvatar" />
    </menu_item_call>
    <menu_item_separator
     layout="topleft" />
    <menu_item_call
     label="Copy Name to clipboard"
     layout="topleft"
     name="url_copy_label">
        <menu_item_call.on_click
         function="Url.CopyLabel" />
    </menu_item_call>
    <menu_item_call
     label="Copy Url to clipboard"
     layout="topleft"
     name="url_copy">
        <menu_item_call.on_click
         function="Url.CopyUrl" />
    </menu_item_call>
</context_menu>
