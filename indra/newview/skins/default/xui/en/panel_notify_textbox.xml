<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<panel
 background_visible="true"
 height="150"
 label="instant_message"
 layout="topleft"
 left="0"
 name="panel_notify_textbox"
 top="0"
 width="305">
    <string
     name="message_max_lines_count"
     value="7" />
    <text_editor
     parse_urls="true"
     enabled="true"
     follows="all"
     height="60"
     layout="topleft"
     left="25"
     max_length="250"
     name="message"
     parse_highlights="true"
     read_only="false"
     top="40"
     type="string"
     use_ellipses="true"
     value="message"
     width="270"
     word_wrap="true" >
    </text_editor>
    <button
     top="110"
     follows="top|left"
     height="20"
     label="Submit"
     layout="topleft"
     left="25"
     name="btn_submit"
     width="70" />
</panel>
