<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<floater
 height="112"
 width="280"
 layout="topleft"
 positioning="centered"
 legacy_header_height="18"
 can_resize="false"
 name="floater_fs_add_contact"
 title="Add to Contact Set...">
	<floater.string name="no_sets">
		No contact sets available.
	</floater.string>
	<floater.string name="text_add_single">
		Please select which contact set to add [NAME] to:
	</floater.string>
	<floater.string name="text_add_multiple">
		Please select which contact set to add [COUNT] avatars to:
	</floater.string>
	<text
	 type="string"
	 follows="left|top"
	 height="35"
	 layout="topleft"
	 left="8"
	 name="textfield"
	 word_wrap="true"
	 top="20"
	 width="265">
	</text>
	<combo_box
	 follows="left|top"
	 layout="topleft"
	 height="23"
	 allow_text_entry="false"
	 top_pad="4"
	 left="8"
	 name="contact_sets"
	 width="239"/>
    <button
     left_pad="2"
     top_delta="0"
     follows="top|right"
     layout="topleft"
     height="24"
	 image_overlay="AddItem_Off"
     name="add_set_btn"
     width="24" />
	<button
	 left="8"
	 top_pad="3"
	 follows="top|right"
	 layout="topleft"
	 height="22"
	 label="Add to set"
	 name="add_btn"
	 width="130" />
	<button
	 left_pad="5"
	 top_delta="0"
	 follows="top|right"
	 layout="topleft"
	 height="22"
	 label="Cancel"
	 name="cancel_btn"
	 width="130" />
</floater>
