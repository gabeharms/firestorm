<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<!-- Width and height of this panel should be synchronized with "panel_modes" in the floater_moveview.xml-->
<panel
 height="21"
 layout="topleft"
 name="panel_stand_stop_flying"
 mouse_opaque="false"
 visible="true"
 width="220">
    <button
     follows="left|bottom"
     height="19"
     label="Stand"
     layout="topleft"
     left="125"
     name="stand_btn"
     tool_tip="Click here to stand up."
     top="2"
     visible="false"
     width="77" />
    <button
     follows="left|bottom"
     height="19"
     label="Stop Flying"
     layout="topleft"
     left="125"
     name="stop_fly_btn"
     tool_tip="Stop flying"
     top="2"
     visible="false"
     width="77" />
    <button
     follows="left|bottom"
     height="19"
     label="Flycam"
     layout="topleft"
     left="80"
     name="flycam_btn"
     tool_tip="Stop Flycam Mode"
     top="2"
     visible="true"
     width="77">
        <commit_callback
         function="View.JoystickFlycam" />
    </button>
</panel>
