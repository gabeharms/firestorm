	<panel
	 bottom="-1"
	 left="3"
	 right="-3"
	 height="24"
	 background_visible="true"
	 follows="left|right|bottom"
	 label="button_bar"
	 layout="topleft"
	 name="button_bar">

		<menu_button
		 top="0"
		 left="0"
		 height="23"
		 width="31"
		 follows="bottom|left"
		 layout="topleft"
		 tool_tip="Show additional options"
		 image_hover_unselected="Toolbar_Left_Over"
		 image_overlay="OptionsMenu_Off"
		 image_selected="Toolbar_Left_Selected"
		 image_unselected="Toolbar_Left_Off"
		 name="sort_options_gear_btn" />

		<button
		 top="0"
		 left_pad="2"
		 height="23"
		 width="31"
		 follows="bottom|left"
		 layout="topleft"
		 image_hover_unselected="Toolbar_Middle_Over"
		 image_overlay="AddItem_Off"
		 image_selected="Toolbar_Middle_Selected"
		 image_unselected="Toolbar_Middle_Off"
		 name="add_btn"
		 tool_tip="Offer friendship to a Resident" />

		<button
		 top="0"
		 left_pad="2"
		 height="23"
		 width="67"
		 follows="bottom|left"
		 label="Profile"
		 layout="topleft"
		 name="view_profile_btn"
		 image_hover_unselected="Toolbar_Middle_Over"
		 image_selected="Toolbar_Middle_Selected"
		 image_unselected="Toolbar_Middle_Off"
		 tool_tip="Show picture, groups, and other Residents information" />

		<button
		 top="0"
		 left_pad="2"
		 width="40"
		 height="23"
		 follows="bottom|left"
		 label="IM"
		 layout="topleft"
		 name="im_btn"
		 image_hover_unselected="Toolbar_Middle_Over"
		 image_selected="Toolbar_Middle_Selected"
		 image_unselected="Toolbar_Middle_Off"
		 tool_tip="Open instant message session" />

		<button
		 top="0"
		 left_pad="2"
		 height="23"
		 width="51"
		 follows="bottom|left"
		 label="Call"
		 layout="topleft"
		 name="call_btn"
		 image_hover_unselected="Toolbar_Middle_Over"
		 image_selected="Toolbar_Middle_Selected"
		 image_unselected="Toolbar_Middle_Off"
		 tool_tip="Call this Resident" />	

		<button
		 top="0"
		 left_pad="2"
		 height="23"
		 width="65"
		 follows="bottom|left"
		 label="Share"
		 layout="topleft"
		 name="share_btn"
		 image_hover_unselected="Toolbar_Middle_Over"
		 image_selected="Toolbar_Middle_Selected"
		 image_unselected="Toolbar_Middle_Off"
		 tool_tip="Share an inventory item" />

		<button
		 top="0"
		 left_pad="2"
		 height="23"
		 width="76"
		 follows="bottom|left"
		 label="Teleport"
		 layout="topleft"
		 name="teleport_btn"
		 image_hover_unselected="Toolbar_Middle_Over"
		 image_selected="Toolbar_Middle_Selected"
		 image_unselected="Toolbar_Middle_Off"
		 tool_tip="Offer teleport" />
		
		
		<dnd_button
		 top="0"
		 left_pad="2"
		 height="23"
		 width="31"
		 follows="bottom|left"
		 image_hover_unselected="Toolbar_Right_Over"
		 image_overlay="TrashItem_Off"
		 image_selected="Toolbar_Right_Selected"
		 image_unselected="Toolbar_Right_Off"
		 layout="topleft"
		 name="del_btn"
		 tool_tip="Remove selected person from your Friends list"/>
		
		<button
		 top="0"
		 left_pad="2"
		 height="23"
		 width="31"
		 follows="bottom|left"
		 image_hover_unselected="Toolbar_Middle_Over"
		 image_overlay="Permission_Visible_Online"
		 image_pressed="PushButton_Press"
		 image_pressed_selected="PushButton_Selected_Press"
		 image_selected="PushButton_Selected_Press"
		 control_name="GlobalOnlineStatusToggle"
		 is_toggle="true"
		 layout="topleft"
		 name="GlobalOnlineStatusToggle"
		 tool_tip="Toggle Global Visibility" />
		
		<icon
		 top="0"
		 left_pad="2"
		 right="-1"
		 height="23"
		 follows="bottom|left|right"
		 layout="topleft"
		 image_name="Toolbar_Middle_Off"
		 name="dummy_icon" />
		
