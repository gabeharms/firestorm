 layout="topleft"
 name="Outfit">
    <menu_item_call
     label="Wear - Replace Current Outfit"
     layout="topleft"
     name="wear_replace">
        <on_click
         function="Outfit.WearReplace" />
    </menu_item_call>
    <menu_item_call
     label="Wear - Add to Current Outfit"
     layout="topleft"
     name="wear_add">
