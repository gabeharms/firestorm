         function="Outfit.TakeOff" />
    </menu_item_call>
    <menu_item_separator />
    <menu_item_call
     label="Rename"
     layout="topleft"
     name="rename">
        <on_click
         function="Outfit.Rename" />
    </menu_item_call>
    <menu_item_call
     label="Delete Outfit"
     layout="topleft"
     name="delete">
        <on_click
         function="Outfit.Delete" />
