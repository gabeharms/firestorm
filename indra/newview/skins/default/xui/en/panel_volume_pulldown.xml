	<button
	 name="mute_audio"
	 left_pad="5"
	 height="16"
	 width="16"
	 follows="top|left"
	 layout="topleft"
	 control_name="MuteAmbient"
	 disabled_control="MuteAudio"
	 image_selected="AudioMute_Off"
	 image_unselected="Audio_Off"
	 is_toggle="true"
	 tab_stop="false"
	/>
	<check_box
	 name="collisions_audio_play_btn"
	 tool_tip="Enable default sounds from collisions"
	 left_pad="5"
	 height="16"
	 width="16"
	 layout="topleft"
	 control_name="FSPlayCollisionSounds"
	 disabled_control="MuteAudio"
	/>
	<slider
	 name="SFX Volume"
	 label="Sounds"
	 top_pad="4"
	 left="10"
	 height="15"
	 width="160"
