	 top_delta="2"
	 left_pad="5"
	 height="16"
	 width="16"
	 follows="left|top"
	 layout="topleft"
	 control_name="EnableVoiceChat"
	 disabled_control="CmdLineDisableVoice"
	 label_text.halign="left"
	/>
</panel>
