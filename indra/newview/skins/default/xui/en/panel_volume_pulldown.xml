<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<panel
 name="volumepulldown_floater"
 height="155"
 width="225"
 follows="bottom"
 layout="topleft"
 background_opaque="true"
 background_visible="true"
 bg_alpha_image="Volume_Background"
 bg_opaque_image="Volume_Background"
 border="false"
 border_visible="false"
 chrome="true"
>
	<slider
	 name="System Volume"
	 label="Master"
	 top="6"
	 left="10"
	 height="15"
	 width="160"
	 follows="top|left"
	 layout="topleft"
	 control_name="AudioLevelMaster"
	 font.style="BOLD"
	 increment="0.025"
	 initial_value="0.5"
	 label_width="60"
	 show_text="false"
	 volume="true"
	>
		<slider.commit_callback
		 function="Pref.setControlFalse"
		 parameter="MuteAudio"
		/>
	</slider>
	<button
	 name="mute_audio"
	 left_pad="5"
	 height="16"
	 width="16"
	 follows="top|left"
	 layout="topleft"
	 control_name="MuteAudio"
	 image_selected="AudioMute_Off"
	 image_unselected="Audio_Off"
	 is_toggle="true"
	 tab_stop="false"
	/>
	<button
	 name="prefs_btn"
	 left_pad="8"
	 height="16"
	 width="16"
	 follows="top|left"
	 layout="topleft"
	 image_disabled="Icon_Gear_Background"
	 image_pressed="Icon_Gear_Press"
	 image_unselected="Icon_Gear_Foreground"
	 scale_image="false"
	>
		<button.commit_callback
		 function="MediaListCtrl.GoMediaPrefs"
		/>
	</button>
	<slider
	 name="UI Volume"
	 label="Buttons"
	 top_pad="6"
	 left="10"
	 height="15"
	 width="160"
	 follows="left|top"
	 layout="topleft"
	 control_name="AudioLevelUI"
	 disabled_control="MuteAudio"
	 increment="0.025"
	 initial_value="0.5"
	 label_width="60"
	 show_text="false"
	 volume="true"
	>
		<slider.commit_callback
		 function="Pref.setControlFalse"
		 parameter="MuteUI"
		/>
	</slider>
	<button
	 name="mute_audio"
	 left_pad="5"
	 height="16"
	 width="16"
	 follows="top|left"
	 layout="topleft"
	 control_name="MuteUI"
	 disabled_control="MuteAudio"
	 image_selected="AudioMute_Off"
	 image_unselected="Audio_Off"
	 is_toggle="true"
	 tab_stop="false"
	/>
	<slider
	 name="Wind Volume"
	 label="Ambient"
	 top_pad="4"
	 left="10"
	 height="15"
	 width="160"
	 follows="left|top"
	 layout="topleft"
	 control_name="AudioLevelAmbient"
	 disabled_control="MuteAudio"
	 increment="0.025"
	 initial_value="0.5"
	 label_width="60"
	 show_text="false"
	 volume="true"
	>
		<slider.commit_callback
		 function="Pref.setControlFalse"
		 parameter="MuteAmbient"
		/>
