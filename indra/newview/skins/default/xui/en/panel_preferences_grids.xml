<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<panel
 border="true"
 follows="all"
 height="440"
 layout="topleft"
 left="1"
 top="1"
 width="540"
 label="Grids"
 name="grids_panel">
	<panel
	 name="layoutfix"
	 layout="topleft"
	 top="0"
	 left="0"
	 height="440"
	 width="540">
	  <!-- Grid Selector -->
	  <text 
		type="string"
		length="1"
		name="grid_selector_label"
		height="15"
		width="100"
		left="30"
		top_pad="10"
		follows="left|top" >
		Select a grid:
	  </text>
	  <combo_box 
		max_chars="20"
		allow_text_entry="false"
		name="grid_selector"
		height="23"
		width="200"
		top_delta="-2"
		left="160" />
	  <button 
		label="Default"
		label_selected="Default"
		name="btn_default"
		height="23"
		width="75"
		left_delta="210"
		halign="center"
		top_delta="0"
		follows="left|top" />
	  <text
		type="string"
		length="1"
		name="default_grid"
		height="15"
		width="150"
		left="370"
		top_pad="5"
		follows="left|top" >
		([DEFAULT])
	  </text>
	  <!-- Buttons -->
	  <button 
		label="Delete"
		label_selected="Delete"
		name="btn_delete"
		height="23"
		width="75"
		left="160"
		top_pad="5"
		halign="center"
		follows="left|top" />
	  <button
		label="New"
		label_selected="New"
		enabled="true"
		name="btn_new"
		height="23"
		width="75"
		left_delta="78"
		top_delta="0"
		halign="center"
		follows="left|top" />
	  <!--
	  <button
		label="Copy"
		label_selected="Copy"
		enabled="true"
		name="btn_copy"
		height="23"
		width="75"
		left_delta="78"
		top_delta="0"
		halign="center"
		follows="left|top" />
	  -->
	  <!-- Advanced -->
	  <button
		label="Advanced"
		label_selected="Advanced"
		name="btn_advanced"
		height="23"
		width="75"
		left_delta="78"
		top_delta="0"
		halign="center"
		follows="left|top" />
	  <!-- Login URI -->
	  <text
		type="string"
		length="1"
		name="loginuri_label"
		height="15"
		width="100"
		left="30"
		top_pad="10"
		follows="left|top">
		Login URI:
	  </text>
	  <line_editor
		max_length="254"
		enabled="true"
		name="loginuri"
		handle_edit_keys_directly="true"
		height="23"
		left="160"
		right="-12"
		top_delta="-5"
		halign="right"
		follows="left|top|right" />
	  <button
		label="Add"
		label_selected="Add"
		name="btn_add"
		height="23"
		width="100"
		left="160"
		top_pad="5"
		halign="center"
		follows="left|top" />
	  <!-- Grid Name -->
	  <text
		type="string"
		length="1"
		name="gridname_label"
		height="15"
		width="100"
		left="30"
		top_pad="10"
		follows="left|top">
		Grid Name:
	  </text>
	  <line_editor
		max_length="254"
		enabled="false"
		name="gridname"
		handle_edit_keys_directly="true"
		height="23"
		left="160"
		right="-12"
		top_delta="-5"
		halign="right"
		follows="left|top|right" />
	  <!-- Grid Message -->
	  <text type="string" length="1" enabled="false" name="gridmessage_label"
			height="15" width="100" left="30" top_pad="8"
			h_pad="0" v_pad="0" halign="left"
			follows="left|top" >
		Grid Info:
	  </text>
	  <text_editor name="gridmessage" type="string" length="1" embedded_items="false" enabled="true" follows="left|top|right"
			 handle_edit_keys_directly="true"
		   height="50" left="161" right="-13" top_delta="-3" max_length="512" />
		   
	  <!-- Advanced settings -->

	  <!-- Grid Splash Page -->
	  <text type="string" length="1" enabled="false" name="loginpage_label"
			height="15" width="100" left="30" top_pad="10" visible="false"
			h_pad="0" v_pad="0" halign="left"
			follows="left|top" >
		Splash Page:
	  </text>
	  <line_editor max_length="254" enabled="false" name="loginpage"
			handle_edit_keys_directly="true" visible="false"
			height="23" left="160" right="-12" top_delta="-5"
			halign="right"
			follows="left|top|right" />
	  <!-- Helper URI -->
	  <text type="string" length="1" enabled="false" name="helperuri_label"
			height="15" width="100" left="30" top_pad="8" visible="false"
			h_pad="0" v_pad="0" halign="left"
			follows="left|top" >
		Helper URL:
	  </text>
	  <line_editor max_length="254" enabled="false" name="helperuri"
			handle_edit_keys_directly="true" visible="false"
			height="23" left="160" right="-12" top_delta="-5"
			halign="right"
			follows="left|top|right" />
	  <!-- Web Site -->
	  <text type="string" length="1" enabled="false" name="website_label"
	  height="15" width="100" left="30" top_pad="8" visible="false"
	  h_pad="0" v_pad="0" halign="left"
	  follows="left|top" >
		Web Site:
	  </text>
	  <line_editor max_length="254" enabled="false" name="website"
	  handle_edit_keys_directly="true" visible="false"
	  height="23" left="160" right="-12" top_delta="-5"
	  halign="right"
	  follows="left|top|right" />
	  <!-- Support URL -->
	  <text type="string" length="1" enabled="false" name="support_label"
	  height="15" width="100" left="30" top_pad="8" visible="false"
	  h_pad="0" v_pad="0" halign="left"
	  follows="left|top" >
		Support URL:
	  </text>
	  <line_editor max_length="254" enabled="false" name="support"
	  handle_edit_keys_directly="true" visible="false"
	  height="23" left="160" right="-12" top_delta="-5"
	  halign="right"
	  follows="left|top|right" />
	  <!-- Account URL -->
	  <text type="string" length="1" enabled="false" name="register_label"
	  height="15" width="100" left="30" top_pad="8" visible="false"
	  h_pad="0" v_pad="0" halign="left"
	  follows="left|top" >
		Account URL:
	  </text>
	  <line_editor max_length="254" enabled="false" name="register"
	  handle_edit_keys_directly="true" visible="false"
	  height="23" left="160" right="-12" top_delta="-5"
	  halign="right"
	  follows="left|top|right" />
	  <!-- Password URL -->
	  <text type="string" length="1" enabled="false" name="password_label"
	  height="15" width="100" left="30" top_pad="8" visible="false"
	  h_pad="0" v_pad="0" halign="left"
	  follows="left|top" >
		Password URL:
	  </text>
	  <line_editor max_length="254" enabled="false" name="password"
	  handle_edit_keys_directly="true" visible="false"
	  height="23" left="160" right="-12" top_delta="-5"
	  halign="right"
	  follows="left|top|right" />
	  <!-- Search URL -->
	  <text type="string" length="1" enabled="false" name="search_label"
	  height="15" width="100" left="30" top_pad="8" visible="false"
	  h_pad="0" v_pad="0" halign="left"
	  follows="left|top" >
		Search URL:
	  </text>
	  <line_editor max_length="254" enabled="false" name="search"
	  handle_edit_keys_directly="true" visible="false"
	  height="23" left="160" right="-12" top_delta="-5"
	  halign="right"
	  follows="left|top|right" />
	  <!-- Rendering Compatibility -->
	  <check_box enabled="true" radio_style="false"
			label="Rendering Compatibility" name="render_compat" visible="false"
			height="23" left="160" top_pad="5"
			h_pad="0" v_pad="0" halign="left"
			follows="left|top" />
	  <button label="?" label_selected="?" enabled="true" name="btn_help_render_compat"
			height="23" width="23" left_delta="205" top_delta="0" visible="false"
			halign="center" follows="left|top" />
	</panel>
</panel>