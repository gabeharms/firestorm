         top="10"
         width="20"
         image_name="TabIcon_Places_Selected"/>
        <text
         follows="all"
         height="90"
         layout="topleft"
         left="10"
         mouse_opaque="false"
         name="tab_description"
         right="-10"
         text_color="white"
         top="40"
         word_wrap="true">
            Find places to go and places you&apos;ve visited before.
        </text>
