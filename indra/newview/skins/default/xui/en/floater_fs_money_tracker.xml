<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<floater
 name="money_tracker"
 help_topic="fs_tip_tracker"
 title="Tip Tracker"
 width="200"
 height="188"
 save_rect="true"
 can_resize="true"
 can_minimize="true"
 can_close="true"
 can_drag_on_left="false">
    <chat_history
    	font="SansSerifSmall"
    	follows="left|right|top|bottom"
    	height="155"
    	name="money_chat_history"
    	parse_highlights="true"
    	parse_urls="true"
    	left="1"
    	width="198">
    </chat_history>
    <button
      	name="Clear"
      	label="Clear"
      	font="SansSerif"
      	mouse_opaque="true"
      	height="20"
      	width="192"
      	left="4"
      	follows="left|right|bottom"/>
</floater>
