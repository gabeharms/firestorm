        <menu
         height="49"
         label="Sort"
         layout="topleft"
         left="0"
         mouse_opaque="false"
         name="Sort"
         tear_off="true"
         top="-113"
         width="118">
            <menu_item_check
             control_name="Inventory.SortByName"
             label="By Name"
             layout="topleft"
             name="By Name">
                <menu_item_check.on_click
                 function="Inventory.SetSortBy"
                 parameter="name" />
            </menu_item_check>
            <menu_item_check
             control_name="Inventory.SortByDate"
             label="By Date"
             layout="topleft"
             name="By Date">
                <menu_item_check.on_click
                 function="Inventory.SetSortBy"
                 parameter="date" />
            </menu_item_check>
            <menu_item_separator
             layout="topleft" />
