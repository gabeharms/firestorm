<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<panel
 border="true"
 follows="all"
 height="440"
 layout="topleft"
 left="1"
 top="1"
 width="540"
 label="Spell Check"
 name="spell_check">
    <check_box
     control_name="SpellCheck"
	 enabled="true"
	 follows="top|left"
	 height="16"
     label="Enable spell checking"
     layout="topleft"
     left="30"
     name="checkSpellCheck"
     top="30"
     width="250" />
    <text
     enabled_control="SpellCheck"
     follows="top|left"
     height="10"
     label="Logs:"
     layout="topleft"
	 left="80"
     mouse_opaque="false"
	 name="textDictionaryMain"
     top_pad="15"
     type="string"
     width="90" >
        Main dictionary :
    </text>
    <combo_box
     enabled_control="SpellCheck"
     follows="top|left"
     height="23"
     layout="topleft"
     left_pad="10"
     name="comboDictionaryMain"
     top_pad="-15"
     width="175" />
</panel>
